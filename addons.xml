<addons>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.CellarDoorTVwizard" name="[COLOR lightskyblue]CellarDoorTV Wizard[/COLOR]" version="0.2.3" provider-name="CDTV Builds">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">[COLOR lightskyblue]CellarDoorTV Wizard[/COLOR]</summary>
    <description lang="en">CellarDoorTV Wizard allows you to get access to the latest CDTV Custom Builds and more.[CR]Credits - A huge thanks goes out to [COLOR deepskyblue]Surfacingx[/COLOR] the creator of the Aftermath Wizard for bringing this outstanding Wizard to this community.[CR]Contact - For help and support go to CellarDoorTV Facebook group.[CR]Follow me on Twitter or visit our website at https://cellardoortv.wordpress.com</description>
    <platform>all</platform>
  </extension>
</addon>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.cdrepo" name="[COLOR steelblue]Cellardoor [COLOR white]Repo[/COLOR]" version="1.0" provider-name="GhostBuilder">
	<extension point="xbmc.addon.repository" name="Cellardoor Repo">
		<info compressed="false">https://raw.githubusercontent.com/CellarDoorTV/repo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/CellarDoorTV/repo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/CellarDoorTV/repo/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>CellarDoorTV Repository</summary>
		<description>Kodi Repository for CellarDoorTV</description>
		<platform>all</platform>
	</extension>
</addon>

<?xml version="1.1" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.hypersonicIPTV2"
       name="[COLOR steelblue]Hypersonic [COLOR white]IPTV2[/COLOR]"
       version="3.3"
       provider-name="GhostBuilder">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.requests" />
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">[COLOR steelblue]Hypersonic [COLOR white]IPTV2[/COLOR]</summary>
    <language>en</language>
    <description lang="en">[COLOR steelblue]Hypersonic[/COLOR] [COLOR white]IPTV[/COLOR] Official Addon - The Future of Great Quality and Stable Streams - for more info go to [COLOR yellowgreen]https://www.hypersonic-tv.com[/COLOR] or email [COLOR yellowgreen]hypersonictvpro@gmail.com[/COLOR] to request your username and password. Due to unexpected volumes, it is taking us a little longer than we would like to respond to emails. Please allow 24hrs for response. Regards, Hypersonic Team.</description>
     <platform>all</platform>
  </extension>
</addon>

</addons>